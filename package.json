{
  "name": "hello-world-fullstack-startos",
  "version": "0.1.0",
  "description": "Hello World Full Stack service for StartOS",
  "scripts": {
    "build": "rm -rf ./javascript && ncc build startos/index.ts -o ./javascript",
    "prettier": "prettier --write .",
    "check": "tsc --noEmit",
    "toshi:install": "cd toshi-moto && pnpm install --frozen-lockfile",
    "toshi:build-ui": "cd toshi-moto && npx nx web-ui:build-umbrel",
    "toshi:build-api": "cd toshi-moto && npx nx api:build"
  },
  "dependencies": {
    "@start9labs/start-sdk": "^0.4.0-beta.41"
  },
  "devDependencies": {
    "@types/node": "^22.18.6",
    "@vercel/ncc": "^0.38.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  }
}